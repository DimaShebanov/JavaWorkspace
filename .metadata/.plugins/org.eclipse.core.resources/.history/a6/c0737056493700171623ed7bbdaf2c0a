package model;
import java.awt.Color;

public class RMData
{
	public int stroke = 5;
	public int number = 0;
	public int type = 3;
	public Color col = Color.black;
	
	
	public RMData(int stroke, int type, Color col, int number)
	{
		this.stroke = stroke;
		this.number = number;
		this.type = type;
		this.col = col;
	}
	
	public RMData()
	{}
	
	public CData dublicate()
	{
		return new CData(stroke, type, col, number);
	}

	
}
